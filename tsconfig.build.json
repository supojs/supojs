{
  "compilerOptions": {
    "target": "ES2021",
    "module": "ES2020",
    "lib": ["ES2021", "DOM"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "sourceMap": true,
    "noEmitOnError": true,
    "skipLibCheck": true,
    "types": ["Node", "jest"]
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
